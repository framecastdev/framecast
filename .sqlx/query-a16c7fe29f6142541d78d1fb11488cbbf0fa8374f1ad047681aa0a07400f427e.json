  "query": "\n            UPDATE users SET\n                credits = credits + $2,\n                updated_at = NOW()\n            WHERE id = $1 AND credits + $2 >= 0\n            RETURNING id, email, name, avatar_url,\n                      tier as \"tier: UserTier\", credits,\n                      ephemeral_storage_bytes, upgraded_at,\n                      created_at, updated_at\n            ",
